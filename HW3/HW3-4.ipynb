    "import math\n",
    "p = 0.6\n",
    "n = 25\n",
    "x = 0\n",
    "for k in range(25//2):\n",
    "    x += math.comb(n,k)*(p**k)*(1-p)**(n-k)\n",
    "print(1-x)"
   ]
